[ "$_CONF_INSTALL_NO_PAGER" = "1" ] && PAGER=cat
: ${_OPTN_INSTALL_BYPASS_UNINSTALL:=1}
which shfmt >/dev/null 2>&1 || _DISABLE_SHFMT=1
